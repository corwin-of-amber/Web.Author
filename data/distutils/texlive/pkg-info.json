{
    "packages": {
        "acmart": {
            "deps": [
                "xkeyval", "xstring", "iftex", "microtype", "etoolbox", "booktabs", "refcount",
                "ltxcmds", "infwarerr", "totpages", "everyshi", "environ", "trimspaces", "setspace",
                "textcase", "natbib", "hyperxmp", "kvoptions", "kvsetkeys", "pdfescape",
                "pdftexcmds", "stringenc", "intcalc", "ifmtarg", "oberdiek", "hyperref",
                "kvdefinekeys", "hycolor", "letltxmacro", "auxhook", "etexcmds", "url", "bitset",
                "bigintcalc", "rerunfilecheck", "uniquecounter", "graphics", "graphics-cfg",
                "graphics-def", "xcolor", "geometry", "ncctools", "cmap", "caption", "float",
                "comment", "fancyhdr", "gettitlestring", "preprint"
            ]
        },
        "latex": {
            "deps": [
                "amsfonts", "cite", "modes", "metafont", "l3kernel", "cm", "amsmath",
                "hyphen-base", "pdftex", "etex", "latex-fonts", "amscls", "l3backend", "plain",
                "tex-ini-files", "latexconfig", "knuth-lib", "ec"
            ]
        },
        "geometry": {
            "deps": ["iftex"]
        },
        "verbatim": {
            "meta": true,
            "deps": ["tools"]
        },
        "xcolor": {
            "deps": ["graphics", "graphics-def"],
            "comment": "actually, xcolor has different deps depending on options... :("
        },
        "wrapfig": {
            "deps": ["collectbox", "iftex", "xkeyval"]
        },
        "tikz": {
            "meta": true,
            "deps": ["pgf", "pgf-blur"]
        },
        "pgf": {
            "deps": ["xstring", "etoolbox", "xcolor", "relsize", "kvoptions", "ltxcmds", "kvsetkeys",
                     "graphics", "graphics-cfg", "graphics-def"]
        },
        "adjustbox": {
            "deps": ["graphics", "graphics-cfg", "graphics-def"]
        },
        "menukeys": {
            "deps": ["l3packages", "pgf"]
        },
        "standalone": {
            "deps": ["iftex", "xkeyval", "tools"]
        }
    }
}