{
  "trailingSlash": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=3600, public"
        }
      ]
    },
    {
      "source": "/(.*).tar.xz",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=84600, public"
        }
      ]
    }
  ]
}
